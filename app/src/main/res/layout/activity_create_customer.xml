                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/tv_register_money"
                    android:text="*"
                    android:textColor="@color/colorRed_FF0000"
                    android:textSize="@dimen/sp_14" />

                <EditText
                    android:id="@+id/et_register_money"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_25"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_register_money"
                    android:background="@null"
                    android:hint="必填"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_main_work"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="主营业务"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color333333"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/tv_main_work"
                    android:text="*"
                    android:textColor="@color/colorRed_FF0000"
                    android:textSize="@dimen/sp_14" />

                <EditText
                    android:id="@+id/et_main_work"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_25"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_main_work"
                    android:background="@null"
                    android:hint="必填"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_principal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="负责人"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color333333"/>

                <EditText
                    android:id="@+id/et_principal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_principal"
                    android:background="@null"
                    android:hint="输入或选择"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:padding="@dimen/dp_10"
                    android:src="@mipmap/person" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_from_department"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="所属部门"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color333333"/>

                <EditText
                    android:id="@+id/et_from_department"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_from_department"
                    android:background="@null"
                    android:hint="输入或选择"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:padding="@dimen/dp_10"
                    android:src="@mipmap/next" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_event_states"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="跟进状态"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color333333"/>

                <EditText
                    android:id="@+id/et_event_states"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_event_states"
                    android:background="@null"
                    android:hint="待跟进"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:padding="@dimen/dp_10"
                    android:src="@mipmap/next" />
            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_40"
                android:text="设置更多"
                android:gravity="center"
                android:layout_gravity="center"
                android:textColor="@color/color999999"
                android:textSize="@dimen/sp_11"
                android:drawablePadding="@dimen/dp_5"
                android:drawableEnd="@mipmap/arrow_expandable"/>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/dp_15"
                android:drawablePadding="@dimen/dp_5"
                android:drawableStart="@mipmap/arrow_expandable"
                android:gravity="center_vertical"
                android:text="联系人信息"
                android:textColor="@color/color333333"
                android:textSize="@dimen/sp_15" />

            <TextView
                android:id="@+id/tv_add_contacts"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:drawablePadding="@dimen/dp_5"
                android:drawableStart="@mipmap/add_contacts"
                android:gravity="center_vertical"
                android:paddingEnd="@dimen/dp_15"
                android:text="添加联系人"
                android:textColor="@color/color4E80F5"
                android:textSize="@dimen/sp_12" />
        </RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_corner_left_right_colorffffff"
            android:orientation="vertical">

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_contacts_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="联系人"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color333333"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/tv_contacts_name"
                    android:text="*"
                    android:textColor="@color/colorRed_FF0000"
                    android:textSize="@dimen/sp_14" />

                <EditText
                    android:id="@+id/et_contacts_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_25"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_contacts_name"
                    android:background="@null"
                    android:hint="请输入"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_contacts_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="电话"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color333333"/>

                <EditText
                    android:id="@+id/et_contacts_phone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_contacts_phone"
                    android:background="@null"
                    android:hint="选填"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_contacts_emails"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="邮箱"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color333333"/>

                <EditText
                    android:id="@+id/et_contacts_emails"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_contacts_emails"
                    android:background="@null"
                    android:hint="选填"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_contacts_department"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="部门"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color333333"/>

                <EditText
                    android:id="@+id/et_contacts_department"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_contacts_department"
                    android:background="@null"
                    android:hint="请选择"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />

