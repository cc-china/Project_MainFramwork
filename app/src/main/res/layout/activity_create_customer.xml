                    android:textSize="@dimen/sp_14" />

                <EditText
                    android:id="@+id/et_register_address"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_25"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_register_address"
                    android:background="@null"
                    android:hint="请选择"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:padding="@dimen/dp_10"
                    android:src="@mipmap/next" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <EditText
                android:id="@+id/et_detail_register_address"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:layout_marginEnd="@dimen/dp_25"
                android:layout_marginStart="@dimen/dp_25"
                android:background="@null"
                android:hint="请输入详细地址，如街道、楼号等"
                android:textColor="@color/color999999"
                android:textSize="@dimen/sp_13" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45">

                <TextView
                    android:id="@+id/tv_business_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_25"
                    android:text="经营地址"
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/tv_business_address"
                    android:text="*"
                    android:textColor="@color/colorRed_FF0000"
                    android:textSize="@dimen/sp_14" />

                <EditText
                    android:id="@+id/et_business_address"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_25"
                    android:layout_marginStart="@dimen/dp_45"
                    android:layout_toEndOf="@+id/tv_business_address"
                    android:background="@null"
                    android:hint="请选择"
                    android:textColor="@color/color999999"
                    android:textSize="@dimen/sp_13" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:padding="@dimen/dp_10"
                    android:src="@mipmap/next" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/colorF0F0F0" />

            <EditText
