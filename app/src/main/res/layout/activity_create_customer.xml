            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_46"
            android:layout_marginEnd="@dimen/dp_40"
            android:layout_marginStart="@dimen/dp_40"
            android:layout_marginTop="@dimen/dp_100"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_add_and_again"
                android:layout_width="@dimen/dp_0"
                android:layout_height="@dimen/dp_40"
                android:layout_weight="7"
                android:background="@drawable/shape_stroke_color4e80f5"
                android:gravity="center"
                android:text="保存并再建一条"
                android:textColor="@color/color4E80F5"
                android:textSize="@dimen/sp_13" />

            <TextView
                android:id="@+id/tv_complete"
                android:layout_width="@dimen/dp_0"
                android:layout_height="@dimen/dp_40"
                android:layout_marginStart="@dimen/dp_25"
                android:layout_weight="4"
                android:background="@drawable/shape_corner_15_color4e80f5"
                android:gravity="center"
                android:text="完成"
                android:textColor="@color/colorFFFFFF"
                android:textSize="@dimen/sp_13" />
        </LinearLayout>
    </LinearLayout>
</ScrollView>