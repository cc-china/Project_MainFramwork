<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relative_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:visibility="gone">


    <com.amap.api.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_179" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_12"
        android:background="@drawable/shape_corner_left_right_colorffffff"
        android:drawableLeft="@mipmap/select_gray"
        android:drawablePadding="@dimen/dp_8"
        android:hint="搜索客户名称、联系人、电话"
        android:paddingBottom="@dimen/dp_5"
        android:paddingEnd="@dimen/dp_15"
        android:paddingStart="@dimen/dp_15"
        android:paddingTop="@dimen/dp_5"
        android:textColorHint="@color/color999999"
        android:textSize="@dimen/sp_13" />
</RelativeLayout>